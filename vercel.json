{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/serverless.ts"
        }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "access-control-allow-origin",
            "value": "*"
          }
        ]
      }
    ]
}